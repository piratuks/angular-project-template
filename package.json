{
  "name": "@piratuks/template",
  "version": "1.0.2",
  "description": "Angular 8 empty project template",
  "author": "Evaldas Laureckas",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "run": "ng serve&&node server.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged&&lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix --project .",
      "git add"
    ]
  },
  "keywords": [
    "Angular project"
  ],
  "license": "MIT",
  "private": true,
  "dependencies": {
